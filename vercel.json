{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/manifest.json", "dest": "/public/manifest.json" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}